            {
              "type": "BuiltinCommonInstructions::ForEachChildVariable",
              "iterableVariableName": "TileMapGenerator.Map.Tiles",
              "valueIteratorVariableName": "T",
              "keyIteratorVariableName": "",
              "conditions": [],
              "actions": [],
              "events": [
                {
                  "type": "BuiltinCommonInstructions::Standard",
                  "conditions": [
                    {
                      "type": {
                        "inverted": false,
                        "value": "VariableChildExists"
                      },
                      "parameters": [
                        "__TileMapGenerator_References",
                        "VariableString(T.Tile)"
                      ],
                      "subInstructions": []
                    }
                  ],
                  "actions": [
                    {
                      "type": {
                        "inverted": false,
                        "value": "ModVarSceneTxt"
                      },
                      "parameters": [
                        "T.Tile",
                        "=",
                        "VariableString(__TileMapGenerator_References[VariableString(T.Tile)])"
                      ],
                      "subInstructions": []
                    }
                  ],
                  "events": []
                },
                {
                  "type": "BuiltinCommonInstructions::JsCode",
                  "inlineCode": "/** @type {gdjs.SpriteRuntimeObject} */\r\nlet tileName = runtimeScene.getVariables().get(\"T\").getChild(\"Tile\").getAsString()\r\nif(tileName.search(\" \") != -1 ){\r\n    tileName = tileName.replace(/\\s/g,\"_\")\r\n    console.log(tileName)\r\n    }\r\nruntimeScene.getVariables().get(\"T\").getChild(\"Tile\").setString(tileName)\r\nconst tileSize = runtimeScene.getVariables().get(\"TileMapGenerator\").getChild(\"Map\").getChild(\"TileSize\").getAsNumber()\r\nlet tile = null \r\ntile = runtimeScene.createObject(tileName)\r\nif( tile !== null ){\r\n    tile.setX(runtimeScene.getVariables().get(\"T\").getChild(\"X\").getAsNumber()* tileSize)\r\n    tile.setY(runtimeScene.getVariables().get(\"T\").getChild(\"Y\").getAsNumber()* tileSize)\r\n    let flipH =  runtimeScene.getVariables().get(\"T\").getChild(\"FlipHorizontally\").getAsBoolean()\r\n    let flipV =  runtimeScene.getVariables().get(\"T\").getChild(\"FlipVertically\").getAsBoolean()\r\n    tile.flipX(flipH)\r\n    tile.flipY(flipV)\r\n }\r\n\r\n\r\n",
                  "parameterObjects": "",
                  "useStrict": true,
                  "eventsSheetExpanded": true
                }
              ]
            },
            {
              "folded": true,
              "type": "BuiltinCommonInstructions::ForEachChildVariable",
              "iterableVariableName": "TileMapGenerator.Map.Items",
              "valueIteratorVariableName": "I",
              "keyIteratorVariableName": "",
              "conditions": [],
              "actions": [],
              "events": [
                {
                  "type": "BuiltinCommonInstructions::Standard",
                  "conditions": [
                    {
                      "type": {
                        "inverted": false,
                        "value": "VariableChildExists"
                      },
                      "parameters": [
                        "__TileMapGenerator_References",
                        "VariableString(I.Item)"
                      ],
                      "subInstructions": []
                    }
                  ],
                  "actions": [
                    {
                      "type": {
                        "inverted": false,
                        "value": "ModVarSceneTxt"
                      },
                      "parameters": [
                        "I.Item",
                        "=",
                        "VariableString(__TileMapGenerator_References[VariableString(I.Item)])"
                      ],
                      "subInstructions": []
                    }
                  ],
                  "events": []
                },
                {
                  "type": "BuiltinCommonInstructions::JsCode",
                  "inlineCode": "let itemName = runtimeScene.getVariables().get(\"I\").getChild(\"Item\").getAsString()\nitemName = itemName.replace(\".png\",\"\")\nitemName = itemName.replace(\".jpg\",\"\")\nif(itemName.search(\" \") != -1 ){\n    itemName = itemName.replace(/\\s/g,\"_\")\n    console.log(itemName)\n    }\nruntimeScene.getVariables().get(\"I\").getChild(\"Item\").setString(itemName)\nlet item = null \nitem = runtimeScene.createObject(itemName)\nif (item !== null){\n    item.setX(runtimeScene.getVariables().get(\"I\").getChild(\"X\").getAsNumber())\n    item.setY(runtimeScene.getVariables().get(\"I\").getChild(\"Y\").getAsNumber())\n}",
                  "parameterObjects": "",
                  "useStrict": true,
                  "eventsSheetExpanded": true
                }
              ]
            }
          ]
        }
